[sip]
root_ca=/share/rootca.pem
verify_server_certs=1
verify_server_cn=1
use_ipv6=0
ipv6_migration_done=1
default_proxy=0
media_encryption=zrtp
inc_timeout=30
in_call_timeout=0
delayed_timeout=4

[rtp]
audio_rtp_port=7078
audio_jitt_comp=60

[sound]
remote_ring=/share/ringback.wav
local_ring=/share/oldphone-mono.wav
echocancellation=1
ec_delay=100
